doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    title User Display
    body
        -var userlist = users
        div
            table.table.table-hover
                thead
                    tr: th Users
                tbody
                    each user in users
                        tr
                            td=user.firstName
                            td=user.lastName
                            td=user.Email
